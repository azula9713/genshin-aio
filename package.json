{
  "private": true,
  "name": "genshin-aio-monorepo",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "client": "yarn workspace client start",
    "server": "yarn workspace server dev",
    "sanity": "yarn workspace @genshin-aio-monorepo/genshin-aio-studio dev",
    "start": "concurrently --kill-others-on-fail \"yarn sanity\" \"yarn server\" \"yarn client\""
  },
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}
